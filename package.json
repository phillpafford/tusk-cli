{
  "name": "tusk-cli",
  "version": "0.1.0",
  "description": "Automate the synchronization of sanitized PostgreSQL schemas and seed data from remote sources into high-fidelity local Docker development environments.",
  "type": "module",
  "main": "bin/tusk.js",
  "bin": {
    "tusk": "./bin/tusk.js"
  },
  "files": [
    "bin/",
    "src/",
    "templates/",
    "infrastructure/",
    "README.md"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "postgres",
    "postgresql",
    "seeding",
    "ddl",
    "faker",
    "docker",
    "devops",
    "database"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/phillpafford/tusk-cli.git"
  },
  "bugs": {
    "url": "https://github.com/phillpafford/tusk-cli/issues"
  },
  "homepage": "https://github.com/phillpafford/tusk-cli#readme",
  "author": "Phillip Pafford",
  "license": "MIT",
  "dependencies": {
    "@faker-js/faker": "^9.3.0",
    "commander": "^13.0.0",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "eslint": "^9.0.0",
    "prettier": "^3.0.0"
  },
  "scripts": {
    "test": "node --test",
    "smoke": "TUSK_CONTRIBUTE=true node bin/tusk.js ops:env:test",
    "lint": "eslint src bin",
    "format": "prettier --write src bin"
  }
}
